{
  "expo": {
    "name": "Clip-it",
    "slug": "clip-it",
    "scheme": "clip-it",
    "description": "My app powered by Draftbit",
    "privacy": "unlisted",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "landscape",
    "icon": "./assets/app/icon.png",
    "splash": {
      "image": "./assets/app/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#5A45FF"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.draftbit.untitledapp",
      "supportsTablet": null,
      "buildNumber": "1.0.0",
      "infoPlist": {},
      "usesIcloudStorage": false
    },
    "android": {
      "versionCode": 1,
      "package": "com.draftbit.untitledapp",
      "permissions": []
    },
    "web": {
      "display": "fullscreen",
      "barStyle": null,
      "themeColor": "#000000",
      "orientation": "natural",
      "backgroundColor": null,
      "preferRelatedApplications": true
    },
    "notification": {
      "androidMode": "default"
    }
  }
}